{
  "$schema": "https://github.com/cloudflare/workers-sdk/files/12887590/wrangler.schema.json",
  "name": "cloudflare-start",
  "main": "worker.ts",
  "compatibility_date": "2025-03-21",
  "workers_dev": true,
  "route": {
    "pattern": "mcp.example.org/*",
    "zone_name": "example.org"
  },
  "env": {
    "staging": {
      "name": "cloudflare-starter-staging",
      "workers_dev": true
    }
  }
}