		"testcontainers": "^11.11.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.0",
		"vite-tsconfig-paths": "^6.0.0",
		"vitest": "^4.0.16",
		"vitest-browser-react": "^2.0.2",
		"wrangler": "^4.54.0"
	},
	"dependencies": {
		"agents": "^0.3.0",
		"better-auth": "^1.4.10",
