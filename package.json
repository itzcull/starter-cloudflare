		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^25.0.0",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/browser-playwright": "^4.0.16",
		"drizzle-kit": "^0.31.8",
